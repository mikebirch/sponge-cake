{
    "name": "mikebirch/sponge-cake",
    "description": "SpongeCake plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "4.4.*",
        "dereuromark/cakephp-tools": "^2.5",
        "cakedc/users": "^11.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SpongeCake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpongeCake\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
