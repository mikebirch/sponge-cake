{
    "name": "mikebirch/sponge-cake",
    "description": "SpongeCake plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "burzum/cakephp-user-tools": "dev-master",
        "dereuromark/cakephp-tools": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SpongeCake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpongeCake\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
